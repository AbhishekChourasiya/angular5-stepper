{
  "name": "angular5-stepper",
  "version": "1.0.0",
  "description": "Angular 5+ stepper module",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "keywords": [
    "angular",
    "angular4",
    "angular5",
    "ng4",
    "ng5",
    "stepper",
    "angular-stepper",
    "angular5-stepper",
    "indrajeet",
    "indra",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "ng",
    "typescript"
  ],
  "author": {
    "name": "Indra Jeet",
    "url": "https://www.linkedin.com/in/indrajeet0510/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/indrajeet0510/angular5-stepper.git"
  },
  "bugs": {
    "url": "https://github.com/indrajeet0510/angular5-stepper/issues"
  },
  "homepage": "https://github.com/indrajeet0510/angular5-stepper#readme",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf coverage dist tmp docs",
    "codecov": "cat coverage/lcov.info | codecov",
    "docs": "compodoc -p tsconfig.json -d docs --disableCoverage",
    "explorer": "source-map-explorer ./dist/index.umd.js",
    "gh-pages": "rimraf docs && npm run docs && gh-pages -d docs",
    "lint": "npm run tslint 'src/**/*.ts'",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf dist tmp",
    "prebuild:watch": "rimraf dist tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "tslint": "tslint"
  },
  "dependencies": {
    "npm": "^5.8.0"
  },
  "peerDependencies": {
    "@angular/common": "^5.2.0",
    "@angular/core": "^5.2.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cli": "~1.7.2",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@ng-bootstrap/ng-bootstrap": "^1.0.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/karma": "^1.7.0",
    "@types/node": "~6.0.60",
    "@types/webpack": "^3.0.13",
    "@types/webpack-env": "^1.13.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "bootstrap": "^4.0.0",
    "codecov": "^2.2.0",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.1",
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-loader": "^0.5.4",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "lodash": "^4.17.5",
    "node-sass": "^4.8.3",
    "protractor": "~5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "sass-loader": "^6.0.5",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3",
    "webpack": "^3.11.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-rxjs-externals": "^1.0.0",
    "zone.js": "^0.8.19"
  }
}
